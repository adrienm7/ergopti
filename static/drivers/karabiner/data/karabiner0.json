                        "manipulators": [
                            {
                                "type": "basic",
                                "from": {
                                    "key_code": "left_shift",
                                    "modifiers": { "optional": ["any"] }
                                },
                                "to": [{ "key_code": "left_shift" }],
                                "to_if_alone": [
                                    {
                                        "key_code": "o",
                                        "modifiers": ["left_command"]
                                    }
                                ]
                            },
                            {
                                "type": "basic",
                                "from": {
                                    "key_code": "fn",
                                    "modifiers": { "optional": ["any"] }
                                },
                                "to_if_alone": [
                                    {

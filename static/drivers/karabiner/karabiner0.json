{
    "profiles": [
        {
            "complex_modifications": {
                "rules": [
                    {
                        "description": "Tab: Cmd+Tab when tapped, Fn when held",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "tab",
                                    "modifiers": { "optional": ["any"] }
                                },
                                "to": [{ "key_code": "fn" }],
                                "to_if_alone": [
                                    {
                                        "key_code": "tab",
                                        "modifiers": ["left_command"]
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Right Command: Tab when tapped, Right Option (AltGr) when held",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "right_command",
                                    "modifiers": { "optional": ["any"] }
                                },
                                "to": [{ "key_code": "right_option" }],
                                "to_if_alone": [{ "key_code": "tab" }],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Special keys end deadkeys and fire immediatly",
                        "manipulators": [
                            {
                                "conditions": [
                                    {
                                        "bundle_identifiers": [
                                            "^com.apple.loginwindow$",
                                            "^com.apple.SecurityAgent$"
                                        ],
                                        "type": "frontmost_application_unless"
                                    }
                                ],
                                "from": { "key_code": "return_or_enter" },
                                "to": [
                                    { "key_code": "keypad_comma" },
                                    { "key_code": "return_or_enter" }
                                ],
                                "type": "basic"
                            },
                            {
                                "conditions": [
                                    {
                                        "bundle_identifiers": [
                                            "^com.apple.loginwindow$",
                                            "^com.apple.SecurityAgent$"
                                        ],
                                        "type": "frontmost_application_unless"
                                    }
                                ],
                                "from": { "key_code": "left_arrow" },
                                "to": [
                                    { "key_code": "keypad_comma" },
                                    { "key_code": "left_arrow" }
                                ],
                                "type": "basic"
                            },
                            {
                                "conditions": [
                                    {
                                        "bundle_identifiers": [
                                            "^com.apple.loginwindow$",
                                            "^com.apple.SecurityAgent$"
                                        ],
                                        "type": "frontmost_application_unless"
                                    }
                                ],
                                "from": { "key_code": "right_arrow" },
                                "to": [
                                    { "key_code": "keypad_comma" },
                                    { "key_code": "right_arrow" }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Tap-hold: left_shift = copy, left_control = paste",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "left_control",
                                    "modifiers": { "optional": ["any"] }
                                },
                                "to_if_alone": [
                                    {
                                        "key_code": "h",
                                        "modifiers": ["left_command"]
                                    }
                                ],
                                "to_if_held_down": [{ "key_code": "left_control" }],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "left_shift",
                                    "modifiers": { "optional": ["any"] }
                                },
                                "to": [{ "key_code": "left_shift" }],
                                "to_if_alone": [
                                    {
                                        "key_code": "o",
                                        "modifiers": ["left_command"]
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Smart mod-tap: left_command = backspace (tap), left_command (hold with other key)",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "left_command",
                                    "modifiers": { "optional": ["any"] }
                                },
                                "parameters": { "basic.to_if_alone_timeout_milliseconds": 200 },
                                "to": [{ "key_code": "left_command" }],
                                "to_if_alone": [{ "key_code": "delete_or_backspace" }],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "(# gives (\" if taped quickly",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "j",
                                    "modifiers": { "mandatory": ["right_option"] }
                                },
                                "to": [
                                    {
                                        "key_code": "j",
                                        "modifiers": ["right_option"]
                                    },
                                    {
                                        "set_variable": {
                                            "name": "altgr_j_pressed",
                                            "value": 1
                                        }
                                    }
                                ],
                                "to_after_key_up": [
                                    {
                                        "set_variable": {
                                            "name": "altgr_j_pressed",
                                            "value": 0
                                        }
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "conditions": [
                                    {
                                        "name": "altgr_j_pressed",
                                        "type": "variable_if",
                                        "value": 1
                                    }
                                ],
                                "from": {
                                    "key_code": "i",
                                    "modifiers": { "mandatory": ["right_option"] }
                                },
                                "to": [
                                    {
                                        "key_code": "c",
                                        "modifiers": ["right_option"]
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "conditions": [
                                    {
                                        "name": "altgr_j_pressed",
                                        "type": "variable_unless",
                                        "value": 1
                                    }
                                ],
                                "from": {
                                    "key_code": "i",
                                    "modifiers": { "mandatory": ["right_option"] }
                                },
                                "to": [
                                    {
                                        "key_code": "i",
                                        "modifiers": ["right_option"]
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    }
                ]
            },
            "devices": [
                {
                    "identifiers": { "is_keyboard": true },
                    "simple_modifications": [
                        {
                            "from": { "key_code": "caps_lock" },
                            "to": [{ "key_code": "return_or_enter" }]
                        },
                        {
                            "from": { "key_code": "right_option" },
                            "to": [
                                { "sticky_modifier": { "right_shift": "toggle" } },
                                { "key_code": "right_shift" }
                            ]
                        }
                    ]
                }
            ],
            "name": "Default profile",
            "selected": true,
            "virtual_hid_keyboard": {
                "country_code": 0,
                "keyboard_type_v2": "ansi"
            }
        }
    ]
}
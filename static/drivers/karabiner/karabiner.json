{
	"global": {
		"check_for_updates_on_startup": true,
		"show_in_menu_bar": true,
		"show_profile_name_in_menu_bar": false
	},
	"profiles": [
		{
			"complex_modifications": {
				"parameters": {
					"basic.simultaneous_threshold_milliseconds": 50,
					"basic.to_delayed_action_delay_milliseconds": 500,
					"basic.to_if_alone_timeout_milliseconds": 1000,
					"basic.to_if_held_down_threshold_milliseconds": 500,
					"mouse_motion_to_scroll.speed": 100
				},
				"rules": [
					{
						"description": "Special keys end deadkeys and fire immediatly",
						"manipulators": [
							{
								"conditions": [
									{
										"bundle_identifiers": ["^com.apple.loginwindow$", "^com.apple.SecurityAgent$"],
										"type": "frontmost_application_unless"
									}
								],
								"from": { "key_code": "return_or_enter" },
								"to": [{ "key_code": "keypad_comma" }, { "key_code": "return_or_enter" }],
								"type": "basic"
							},
							{
								"conditions": [
									{
										"bundle_identifiers": ["^com.apple.loginwindow$", "^com.apple.SecurityAgent$"],
										"type": "frontmost_application_unless"
									}
								],
								"from": { "key_code": "left_arrow" },
								"to": [{ "key_code": "keypad_comma" }, { "key_code": "left_arrow" }],
								"type": "basic"
							},
							{
								"conditions": [
									{
										"bundle_identifiers": ["^com.apple.loginwindow$", "^com.apple.SecurityAgent$"],
										"type": "frontmost_application_unless"
									}
								],
								"from": { "key_code": "right_arrow" },
								"to": [{ "key_code": "keypad_comma" }, { "key_code": "right_arrow" }],
								"type": "basic"
							}
						]
					},
					{
						"description": "Change spacebar to left_shift. (Post spacebar if pressed alone)",
						"manipulators": [
							{
								"from": {
									"key_code": "spacebar",
									"modifiers": {
										"optional": ["any"]
									}
								},
								"to": [
									{
										"key_code": "left_shift"
									}
								],
								"to_if_alone": [
									{
										"key_code": "spacebar"
									}
								],
								"type": "basic"
							}
						]
					},
					{
						"description": "Tap-hold: left_control = paste, left_shift = copy",
						"manipulators": [
							{
								"type": "basic",
								"from": {
									"key_code": "left_control",
									"modifiers": {
										"optional": ["any"]
									}
								},
								"to_if_alone": [
									{
										"key_code": "h",
										"modifiers": ["left_command"]
									}
								],
								"to_if_held_down": [
									{
										"key_code": "left_control"
									}
								]
							},
							{
								"type": "basic",
								"from": {
									"key_code": "left_shift",
									"modifiers": {
										"optional": ["any"]
									}
								},
								"to_if_alone": [
									{
										"key_code": "o",
										"modifiers": ["left_command"]
									}
								],
								"to_if_held_down": [
									{
										"modifier_flag": "shift"
									}
								]
							}
						]
					},
					{
						"description": "Smart mod-tap: left_command = backspace (tap), left_command (hold with other key)",
						"manipulators": [
							{
								"type": "basic",
								"from": {
									"key_code": "left_command",
									"modifiers": {
										"optional": ["any"]
									}
								},
								"parameters": {
									"basic.to_if_alone_timeout_milliseconds": 200
								},
								"to_if_alone": [
									{
										"key_code": "delete_or_backspace"
									}
								],
								"to": [
									{
										"key_code": "left_command"
									}
								]
							}
						]
					},
					{
						"description": "Right_option becomes left_shift",
						"manipulators": [
							{
								"type": "basic",
								"from": {
									"key_code": "right_option",
									"modifiers": {
										"optional": ["any"]
									}
								},
								"to": [
									{
										"key_code": "left_shift"
									}
								]
							}
						]
					},
					{
						"description": "Right_command becomes right_option (AltGr)",
						"manipulators": [
							{
								"type": "basic",
								"from": {
									"key_code": "right_command",
									"modifiers": {
										"optional": ["any"]
									}
								},
								"to": [
									{
										"key_code": "right_option"
									}
								]
							}
						]
					}
				]
			},
			"devices": [],
			"fn_function_keys": [
				{
					"from": {
						"key_code": "f1"
					},

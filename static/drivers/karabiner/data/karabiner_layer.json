{
	"description": "Left Command becomes a navigation layer",
	"manipulators": [
		{
			"description": "Hold: activate layer temporarily | Tap: send BackSpace",
			"from": { "key_code": "left_command" },
			"to": [
				{
					"set_variable": {
						"name": "layer_hold",
						"value": 1
					}
				},
				{
					"set_variable": {
						"name": "layer_active",
						"value": 1
					}
				},
				{ "key_code": "f19" }
			],
			"to_after_key_up": [
				{
					"set_variable": {
						"name": "layer_hold",
						"value": 0
					}
				},
				{
					"conditions": [
						{
							"name": "layer_toggle",
							"type": "variable_unless",
							"value": 1
						}
					],
					"set_variable": {
						"name": "layer_active",
						"value": 0
					}
				},
				{ "key_code": "f19" }
			],
			"to_if_alone": [{ "key_code": "delete_or_backspace" }],
			"type": "basic"
		},
